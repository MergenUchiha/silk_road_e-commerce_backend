{
    "name": "clicker",
    "version": "0.0.1",
    "description": "",
    "author": "github.com/MergenUchiha",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prisma:generate": "prisma generate  --schema ./src/prisma/schema",
        "prisma:migrate": "prisma migrate dev --preview-feature --schema ./src/prisma/schema",
        "prisma:deploy": "prisma migrate deploy --preview-feature --schema ./src/prisma/schema",
        "prisma:generate:dev": "dotenv -e .development.env -- prisma generate --schema ./src/prisma/schema",
        "prisma:format:dev": "dotenv -e .development.env -- prisma format --schema ./src/prisma/schema",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "prisma:migrate:dev": "dotenv -e .development.env -- prisma migrate dev --preview-feature --schema ./src/prisma/schema",
        "update": "npx npm-check --update",
        "build": "nest build",
        "seed": "tsc && dotenv -e .development.env --  node dist/helpers/seeds/seed.js",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start:dev": "cross-env NODE_ENV=development dotenv -e .development.env -- concurrently \"tsc -w\" \"nest start --watch\"",
        "start:test": "nest start -b swc --type-check --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "dotenv -e .env -- nest start",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json --watchAll --detectOpenHandles --maxWorkers=6",
        "sentry:sourcemaps": "sentry-cli sourcemaps inject --org david-b8ed80b4b --project p2p_bot ./ && sentry-cli sourcemaps upload --org david-b8ed80b4b --project p2p_bot ./"
    },
    "dependencies": {
        "@faker-js/faker": "^10.1.0",
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^11.0.1",
        "@fastify/csrf-protection": "^7.1.0",
        "@fastify/helmet": "^13.0.1",
        "@fastify/multipart": "^9.0.3",
        "@keyv/redis": "4.2.0",
        "@nestjs/axios": "4.0.0",
        "@nestjs/cache-manager": "3.0.0",
        "@nestjs/common": "^11.1.1",
        "@nestjs/config": "4.0.0",
        "@nestjs/core": "^11.1.1",
        "@nestjs/platform-express": "11.1.1",
        "@nestjs/platform-fastify": "^11.1.1",
        "@nestjs/swagger": "11.0.4",
        "@nestjs/terminus": "11.0.0",
        "@nestjs/throttler": "6.4.0",
        "@prisma/client": "6.4.1",
        "@sentry/nestjs": "9.1.0",
        "argon2": "0.41.1",
        "class-transformer": "0.5.1",
        "compression": "1.8.0",
        "cross-env": "7.0.3",
        "date-fns": "4.1.0",
        "dotenv": "16.4.7",
        "dotenv-cli": "8.0.0",
        "fastify-file-interceptor": "1.0.9",
        "fastify-multer": "2.0.3",
        "ioredis": "5.5.0",
        "jsonwebtoken": "9.0.2",
        "minio": "8.0.4",
        "multer": "1.4.5-lts.1",
        "nestjs-zod": "4.3.1",
        "reflect-metadata": "0.2.2",
        "rxjs": "7.8.2",
        "typescript-eslint": "8.29.1",
        "uuid": "11.1.0",
        "winston": "3.17.0",
        "zod": "3.24.2"
    },
    "devDependencies": {
        "@nestjs/cli": "11.0.4",
        "@nestjs/schematics": "11.0.1",
        "@types/express": "5.0.1",
        "@types/jsonwebtoken": "9.0.9",
        "@types/multer": "1.4.12",
        "@types/node": "^22.15.18",
        "concurrently": "9.1.2",
        "eslint": "9.21",
        "eslint-config-prettier": "10.0.1",
        "eslint-plugin-import": "2.31.0",
        "eslint-plugin-module-resolver": "1.5.0",
        "eslint-plugin-prettier": "5.2.3",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "fastify": "^5.3.3",
        "jest": "29.7.0",
        "prettier": "3.5.2",
        "prisma": "6.4.1",
        "supertest": "7.0.0",
        "ts-jest": "29.2.6",
        "ts-loader": "9.5.2",
        "ts-node": "10.9.2",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.7.3",
        "webpack": "5.98.0"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "moduleDirectories": [
            "node_modules",
            "src"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
